:> "Step by 1\n";
(i = 1; i <= 9; i++) {
    :> i * (3 + i);:> "\n";
}

:> "Step by 2\n";
(i = 1; i < 9; i+=2) {
    :> i * (3 + i);:> "\n";
}

q = 5;
(; q > 1; q--) {
    :> "Rebel ${q}";
    :> "\n";
}

q = 3;
(; q > 1;) {
    :> "Boom ... ${q}";
    :> "\n";

    q--;
}

